<h1>All Articles</h1>

<ul id="articles">
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article) %>
    </li>
  
</ul>

<%= link_to article.title, article_path(article), class: 'article_title' %>

<% end %>

<%= link_to "Create New Article", new_article_path%>